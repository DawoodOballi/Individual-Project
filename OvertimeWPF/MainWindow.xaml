<Window x:Class="OvertimeWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OvertimeWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#FFD3C035" Offset="1"/>
            <GradientStop Color="#FFFF6500"/>
        </LinearGradientBrush>
    </Window.Background>
    <Viewbox Stretch="Uniform" StretchDirection="Both">
        <Frame Name="frame" NavigationUIVisibility="Hidden" Source="/LogInPage.xaml"/>
    </Viewbox>
</Window>
